        dataType: data_type,
        success: function (data) {

            var countData = _.size(data);

    //

            var mainCount = 0;
            var comments = [];
            var parentID = "";
            $.each( data, function( key, value ) {

                mainCount++;
                var comment =  createSubCommentHtml(value,mainCount);
                comments.push(comment);
                parentID = value.parent_ID;
